{
  "name": "egg-mysql",
  "version": "3.4.0",
  "description": "MySQL plugin for egg",
  "eggPlugin": {
    "name": "mysql"
  },
  "files": [
    "config",
    "lib",
    "agent.js",
    "app.js",
    "index.d.ts"
  ],
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "mysql",
    "database"
  ],
  "dependencies": {
    "ali-rds": "^4.0.1"
  },
  "devDependencies": {
    "egg": "^3.5.0",
    "egg-bin": "^5.5.0",
    "egg-ci": "^2.1.0",
    "egg-mock": "^5.2.1",
    "eslint": "^8.16.0",
    "eslint-config-egg": "^12.1.0",
    "git-contributor": "^1.0.10",
    "tsd": "^0.25.0",
    "utility": "^1.17.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "contributor": "git-contributor",
    "tsd": "tsd",
